{
    "manifest_version": 2,
    "name": "ScrollNt",
    "version": "0.0.0.1",
    "permissions": ["tabs", "notifications", "webNavigation"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
    },
    "browser_action": {
      "default_icon": "128.png",
      "default_title": "Click here to open menu",
      "default_popup": "menu.html"
    }
  }
  